/* ---------------------------------------------
*   global-nav
--------------------------------------------- */
.global-nav__list {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-pack: center;
    -ms-flex-align: center;

    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}

.global-nav__list-item {
    margin-right: 15px;
}

.global-nav__list-item:last-child {
    margin-right: 0;
}

.global-nav__list-item--frame {
    margin-right: auto;
    width: 100px;
    text-align: center;
}

.global-nav__list-item--frame .global-nav__link-inner {
    padding: 54px 0 5px;
}

.global-nav__list-item--narrow {
    padding: 28px 17px;
}

.global-nav__list-item--icon .global-nav__link-inner {
    position: relative;
}

.global-nav__list-item--icon .global-nav__link-inner:before {
    position: absolute;
    top: 33px;
    left: 50%;
    display: block;
    background-position: 50% 0;
    background-size: 100% auto;
    background-repeat: no-repeat;
    content: "";
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}

.global-nav__list-item--icon-search .global-nav__link-inner {
    border-left: 1px solid #E5E5E5;
}

.global-nav__list-item--icon-search .global-nav__link-inner:before {
    width: 21px;
    height: 22px;
    background-image: url(/img/common/icon_search.png);
}

.global-nav__list-item--icon-sitemap .global-nav__link-inner {
    background-color: #E5E5E5;
}

.global-nav__list-item--icon-sitemap .global-nav__link-inner:before {
    width: 29px;
    height: 27px;
    background-image: url(/img/common/icon_sitemap.png);
}

.global-nav__list-item--icon-english .global-nav__link-inner {
    background-color: #427C9D;
    color: #fff;
}

.global-nav__list-item--icon-english .global-nav__link-inner:before {
    width: 24px;
    height: 24px;
    background-image: url(/img/common/icon_english.png);
}

@media screen and (max-width: 767px) {
    .global-nav__list-item--icon-english .global-nav__link-inner:before {
        top: 20px;
        width: 15px;
    }
}

.global-nav__list-item--icon-contact .global-nav__link-inner {
    background-color: #212A4B;
    color: #fff;
}

.global-nav__list-item--icon-contact .global-nav__link-inner:before {
    width: 26px;
    height: 18px;
    background-image: url(/img/common/icon_mail.png);
}

@media screen and (max-width: 767px) {
    .global-nav__list-item--icon-contact .global-nav__link-inner:before {
        top: 20px;
        width: 15px;
    }
}

.global-nav__link {
    display: block;
    font-weight: bold;
    font-size: 14px;
    -webkit-transition: opacity 300ms;
    transition: opacity 300ms;
}

.global-nav__link:hover {
