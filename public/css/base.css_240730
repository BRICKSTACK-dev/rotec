/* english
------------------------------------------------------ */
.chance_eng {
    text-align: right;
}

.chance_jap {
    padding-top: 50px;
    text-align: right;
}

.bl_fadein {
    opacity: 0;
    -webkit-transition: all 300ms;
    transition: all 300ms;
    -webkit-transform: translate(0, 50px);
    transform: translate(0, 50px);
}

.bl_fadein.bl_scrollin {
    opacity: 1;
    -webkit-transform: translate(0, 0);
    transform: translate(0, 0);
}

@media all and (min-width: 0) and (max-width: 767px) {
    .bl_fadein {
        opacity: 1;
        -webkit-transition: initial;
        transition: initial;
        -webkit-transform: initial;
        transform: initial;
    }
}

/*  footer-pagetop
--------------------------------------------- */
@media screen and (min-width: 751px) {
    .footer-pagetop {
        position: relative;
    }
}

.js-target {
    position: fixed;
    right: 30px;
    bottom: 0;
    z-index: 8;
    display: block;
    overflow: hidden;
    width: 66px;
    height: 66px;
    opacity: 0;
}

.js-target--english {
    bottom: 120px;
}

@media screen and (max-width: 750px) {
    .js-target {
        right: 10px;
        width: 33px;
        height: 33px;
    }
}

.js-target--absolute {
    position: absolute;
    bottom: auto;
}

.js-target.is-show {
    opacity: 1;
    -webkit-transition: opacity 300ms;
    transition: opacity 300ms;
}

@media screen and (min-width: 751px) {
    .js-target.is-show:hover {
        opacity: 0.8;
    }
}

