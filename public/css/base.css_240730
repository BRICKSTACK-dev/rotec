/* ==========================================================
Name:
    base.css

Description:
    サイト全体に共通する設定を記述する

Contents:
    base settings
    javascript style hooks
    base blocks
========================================================== */
/* ==========================================================
*
*   base settings
*
========================================================== */
/* ---------------------------------------------
*   Rounded Mplus 1c
--------------------------------------------- */
@import url("https://fonts.googleapis.com/css?family=Lato:400,700");
/* ---------------------------------------------
*   html, body
--------------------------------------------- */
html,
body {
    color: #333;
    font-size: 16px;
    font-family: "Lato", "Yu Gothic Medium", "游ゴシック Medium", YuGothic, "游ゴシック体", "ヒラギノ角ゴ Pro W3", "メイリオ", sans-serif;

    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    -webkit-text-size-adjust: 100%;
}

@media screen and (max-width: 767px) {
    html,
    body {
        font-size: 2.4vw;
    }
}

body {
    min-width: 1220px;
    line-height: 1.5;
}

@media screen and (max-width: 767px) {
    body {
        position: relative;
        min-width: 320px;
        line-height: 1.5;
    }
}

/* ---------------------------------------------
*   <a> tag
--------------------------------------------- */
a {
    color: inherit;
    text-decoration: none;
}

/* ---------------------------------------------
*   <img> tag
--------------------------------------------- */
img {
    max-width: 100%;
    width: auto;
    height: auto;
    vertical-align: bottom;
}

/* ==========================================================
*
*   javascript style hooks
*
========================================================== */
/* ==========================================================
*
*   base blocks
*
========================================================== */
/* ---------------------------------------------
*   header
--------------------------------------------- */
.header {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 1;
    min-width: 1220px;
    width: 100%;
    background-color: #fff;
}

@media screen and (max-width: 767px) {
    .header {
        min-width: auto;
    }
}

/*  header-container
--------------------------------------------- */
.header-container {
    position: relative;
}

/*  header-logo
--------------------------------------------- */
.header-logo {
    margin-left: 7px;
}

.header-logo__link {
    display: block;
    overflow: hidden;
    padding-top: 71px;
    width: 179px;
    height: 0;
    background: url(/img/common/header_logo.png) 0 0 no-repeat;
    background-size: contain;
    background-repeat: no-repeat;
}

@media screen and (max-width: 767px) {
    .header-logo__link {
        padding-top: 32px;
        width: 79px;
    }
}

.header-logo--english .header-logo__link {
    padding-top: 63px;
    width: 355px;
    background: url(/img/english/logo_en_head.png) 0 0 no-repeat;
}

@media screen and (max-width: 767px) {
    .header-logo--english .header-logo__link {
        padding-top: 35.4929578px;
        width: 200px;
        background-size: 100% auto;
    }
}

/*  header-menu
--------------------------------------------- */
.header-menu {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-pack: justify;
    -ms-flex-align: center;
    margin-right: auto;
    margin-left: auto;
    padding: 0 43px;
    max-width: 1200px;
    background-color: #fff;

    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

@media screen and (max-width: 767px) {
    .header-menu {
        padding: 0;
        max-width: none;
        height: 40px;
    }
}

/*  header-sp-trigger
--------------------------------------------- */
.header-sp-trigger {
    display: none;
}

